<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="adminBorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.adminPageController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <HBox fx:id="adminTopHbox" prefHeight="65.0" prefWidth="1300.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="47.0" prefWidth="900.0" />
            <Button fx:id="settingsButton" maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" prefWidth="120.0" stylesheets="@style.css" text="SETTINGS">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="logoutButton" maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" prefWidth="120.0" stylesheets="@style.css" text="LOG OUT">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <left>
      <VBox fx:id="adminLeftVbox" prefHeight="475.0" prefWidth="162.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addUserButton" mnemonicParsing="false" onAction="#initialize" prefWidth="150.0" stylesheets="@style.css" text="ADD USER">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="removeUserButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="REMOVE USER">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="inventoryButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="INVENTORY">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="ordersButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="ORDERS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="batchButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="BATCH">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="debitsButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="DEBITS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="accountsButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="ACCOUNTS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="basicButton" mnemonicParsing="false" prefWidth="150.0" stylesheets="@style.css" text="BASIC">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <bottom>
      <HBox prefHeight="32.0" prefWidth="880.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <GridPane fx:id="adminCenter" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </center>
</BorderPane>
